{
  "name": "browse-vscode",
  "displayName": "browse",
  "private": true,
  "description": "Add VSCode support for the browse language",
  "version": "0.0.1",
  "publisher": "pranaygp",
  "author": "Windsor Team <oss@windsor.io>",
  "homepage": "https://github.com/windsorio/browse#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windsorio/browse.git"
  },
  "bugs": {
    "url": "https://github.com/windsorio/browse/issues"
  },
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "browse",
        "extensions": [
          ".browse"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "browse",
        "scopeName": "source.browse",
        "path": "./syntaxes/browse.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.css": "css",
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ]
  }
}
